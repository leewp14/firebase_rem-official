{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, no-cache"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "*",
        "destination": "https://rem-official.github.io/",
        "type": 302
      }
    ]
  }
}